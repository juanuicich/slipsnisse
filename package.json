{
  "name": "slipsnisse",
  "version": "1.0.0",
  "description": "MCP server that exposes AI-powered composite tools",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "slipsnisse": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --write ."
  },
  "keywords": [
    "mcp",
    "ai",
    "tools",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.18",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/openai": "^3.0.14",
    "@ai-sdk/openai-compatible": "^2.0.16",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "ai": "^6.0.45",
    "pino": "^10.2.1",
    "pino-pretty": "^13.1.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
